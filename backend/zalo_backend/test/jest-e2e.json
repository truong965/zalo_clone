{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "..",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/src/$1",
    "^@paralleldrive/cuid2$": "<rootDir>/test/mocks/cuid2.mock.ts",
    "^file-type$": "<rootDir>/test/mocks/file-type.mock.ts"
  },
  "transformIgnorePatterns": [
    "/node_modules/(?!api-query-params)"
  ]

}
