  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "vitest run",
    "test:e2e": "vitest run --config vitest.config.e2e.ts",
    "test:e2e:ui": "vitest --ui --config vitest.config.e2e.ts",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "prisma:generate": "dotenv -e .env.development.local -- npx prisma generate",
    "prisma:migrate": "dotenv -e .env.development.local -- npx prisma migrate dev",
    "prisma:studio": "dotenv -e .env.development.local -- npx prisma studio",
    "test:load": "artillery run test/load-tests/scenarios/basic-connection.yml",
    "test:load:connection-churn": "artillery run test/load-tests/scenarios/connection-churn.yml",
    "test:load:message-flood": "artillery run test/load-tests/scenarios/message-flood.yml",
    "test:load:slow-client": "artillery run test/load-tests/scenarios/slow-client.yml",
    "test:load:all": "npm run test:load && npm run test:load:connection-churn && npm run test:load:message-flood"
  },
